@using OnlineTicariOtomasyon.Models.Siniflar
@model List<KargoTakip>
@{
    ViewBag.Title = "CariKargoTakip";
    Layout = "~/Views/Shared/_CariLayout.cshtml";
}

<h2>Kargo Takip</h2>
<br />
<table class="table table-striped">
    <tr>
        <th>Açıklama</th>
        <th>Tarih</th>

    </tr>
    @foreach (var k in Model)
    {
        <tr>
            <td>@k.Aciklama</td>
            <td>@k.TarihZaman</td>

        </tr>

    }

</table>
